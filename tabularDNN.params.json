{
    "description": "Trying 16 nodes in the first hidden layer",
    "params": {
        "input_features": [
            "Date",
            "Time",
            "Temperature",
            "Real Feel",
            "Weather Status",
            "Wind Gusts",
            "Humidity",
            "Indoor Humidity",
            "Humidity Status",
            "Dew Point",
            "Pressure",
            "Pressure Direction",
            "Cloud Cover",
            "Visibility",
            "Cloud Ceiling"
